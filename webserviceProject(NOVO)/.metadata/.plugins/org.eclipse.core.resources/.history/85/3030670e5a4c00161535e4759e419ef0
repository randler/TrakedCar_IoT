package br.edu.ifba.mobile.andro.esteira.bean;

public class Usuario {
	
	private int cpf;
	private String nome;
	
	public int getCpf() {
		return cpf;
	}
	public void setCpf(int cpf) {
		this.cpf = cpf;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public String toString(){
		return "CPF: "+cpf+ "\nNome: "+ nome;
	}

}
